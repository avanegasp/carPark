<%= form_for @vehicle, :html =>{:class => "form"}, remote: true do |f|%>
  <div class="form-group">
    <%= f.label :plate%>
    <%= f.text_field :plate, class:"form-control"%>
  </div>
  <div class="form-group">
    <%= f.label :vehicle_type %>
    <%= f.text_field :vehicle_type, class:"form-control"%>
  </div>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
